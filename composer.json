{
    "name": "anime-db/ani-db-filler-bundle",
    "license": "GPL-3.0",
    "type": "anime-db-plugin",
    "description": "Plugin AniDB for the catalog Anime DB",
    "homepage": "http://github.com/anime-db/ani-db-filler-bundle",
    "autoload": {
        "psr-4": {
            "AnimeDb\\Bundle\\AniDbFillerBundle\\": "src/",
            "AnimeDb\\Bundle\\AniDbBrowserBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "anime-db/ani-db-browser-bundle": "~1.0.0"
    },
    "require-dev": {
        "anime-db/app-bundle": "~0.4.4",
        "anime-db/catalog-bundle": "~0.4.26",
        "symfony/expression-language": "~2.4|~3.0",
        "phpunit/phpunit": "^4.8.0",
        "scrutinizer/ocular": "~1.3.0",
        "satooshi/php-coveralls": "^1.0.0"
    },
    "minimum-stability": "dev"
}
